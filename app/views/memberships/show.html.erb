<div class="page-header">
  <h3>Membership #<%= @membership.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Group</dt>
      <dd><%= @membership.group_id %></dd>

      <dt>Member</dt>
      <dd><%= @membership.member_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/memberships" class="btn btn-primary">
        Back
      </a>
      <a href="/memberships/<%= @membership.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_membership/<%= @membership.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
